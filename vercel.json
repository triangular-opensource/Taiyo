{
  "builds": [
      {
          "src": "Taiyo/wsgi.py",
          "use": "@vercel/python"
      },
      {
            "src": "build_files.sh",
            "use": "@vercel/static",
            "config": { "distDir": "staticfiles_build" }
      }
  ],

  "routes": [
      {
          "src": "/(.*)",
          "dest": "Taiyo/wsgi.py"
      }
    ]
}